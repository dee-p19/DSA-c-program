#include<stdio.h>
void main ()
{
  int a[]={3,5,2,37,45,12,90};
  int count=0,count1=0,count2=0;
    for(int i=0;i<7;i++)
    {
      if (a[i]%3==0)
       count1++;
      if (a[i]%5==0)
       count2++;
      if (a[i]%3==0 && a[i]%5==0)
       count++;
    }
    printf("divisible by 3=%d\n",count1);
    printf("divisible by 5=%d\n",count2);
    printf("divisible by 3 and 5=%d\n",count);
}


#include <stdio.h>
void main() {
    int a[]={3,5,2,0,0,3,0,7,0,45,12,90};
    int a1[12];
    int i,j=0,count=0;
    for(i=0;i<12;++i){
        if (a[i]==0)
            count ++;
    }
    for( i=0;i<12;i++){
        if (a[i]!=0){
            a1[j]=a[i];
            j++;
        }
    }
    for(i=j;i<12;i++)
        {a1[i]=0;}
        
    for(i=0;i<12;i++)
        printf("%d ",a1[i]);
}
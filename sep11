#include<stdio.h>
void main()
{
  int n;
  printf("enter a number");
  scanf("%d",&n);
  switch(n/10)
    {
        case 9:
        case 10:
             printf("A grade");
             break;
        case 8:
             printf("B grade");
             break;
        case 7:
             printf("C grade");
             break;
        case 6:
             printf("D grade");
             break;
        case 5:
             printf("E grade");
             break;
        default:
             printf("fail");
    }
}

#include<stdio.h>
void main()
{
  int n;
  printf("enter an operator");
  scanf("%d",&n);
  switch(n/10)
    {
        case 9:
        case 10:
             printf("A grade");
             break;
        case 8:
             printf("B grade");
             break;
        case 7:
             printf("C grade");
             break;
        case 6:
             printf("D grade");
             break;
        case 5:
             printf("E grade");
             break;
        default:
             printf("fail");
    }
}

